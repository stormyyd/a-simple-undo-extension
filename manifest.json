{
    "manifest_version": 2,
    "name": "Undo",
    "version": "0.0.1",
  
    "description": "A simple undo extension.",
    "homepage_url": "https://github.com/stormyyd",
    "icons": {
      "48": "icons/undo.svg",
      "96": "icons/undo.svg"
    },
  
    "permissions": [
      "storage",
      "tabs"
    ],
  
    "browser_action": {
      "default_icon": "icons/undo.svg",
      "default_title": "Undo",
      "default_popup": "html/popup.html"
    },

    "background": {
        "scripts": ["js/background.js"]
    }
}
